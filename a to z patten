#include <stdio.h>

int main()
{
    int i,j;
    for(i=1;i<=4;i++)
    {
        for(j=1;j<=5;j++)
        {
            if(i==1||j==3)
            printf("* ");
    
        else
        printf("  ");
        }
    printf("\n");
    }
}

